{
  "name": "Requests",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Extension to extract requests",
  "permissions": [
    "activeTab",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "images/worksheet-16.png",
    "32": "images/worksheet-32.png",
    "128": "images/worksheet-128.png",
    "512": "images/worksheet-512.png"
  },
  "declarative_net_request":{
    "rule_resources":[{
      "id": "rules_1",
      "enabled": true,
      "path": "rules.json"
    }]
  },
  "devtools_page": "devtools.html"
}